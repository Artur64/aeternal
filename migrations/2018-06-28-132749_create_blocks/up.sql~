CREATE TABLE blocks (
       id SERIAL PRIMARY KEY,
       hash VARCHAR(55),
       height UNSIGNED BIG INTEGER,
       miner VARCHAR(55),
       nonce UNSIGNED BIG INTEGER,
       prev_hash VARCHAR(55),
       state_hash VARCHAR(55),
       target UNSIGNED BIG INTEGER,
       time_ UNSIGNED BIG INTEGER,
       txs_hash VARCHAR(255),
       version INTEGER);
